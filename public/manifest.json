{
    "name": "new-tab",
    "version": "0.0.1",
    "description": "Replaces the new-tab page with a useful alternative.",
    "homepage_url": "https://github.com/AlexanderKosnac/new-tab",
    "manifest_version": 3,

    "chrome_url_overrides": {
        "newtab": "index.html"
    },

    "action": {
        "default_popup": "popup.html"
    },

    "permissions": [
        "storage"
    ],

    "icons": {
        "48": "favicon.svg",
        "96": "favicon.svgg"
    }
}